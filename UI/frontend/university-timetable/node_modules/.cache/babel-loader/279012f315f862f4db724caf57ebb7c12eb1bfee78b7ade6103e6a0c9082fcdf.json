  instructor: '',\n      responseMessage: '',\n      responseOutput: '',\n    };\n  },\n  methods: {\n    async addCourse() {\n      try {\n        const response = await axios.post('http://localhost:5000/addCourse', {\n          courseName: this.courseName,\n          timeSlotCourse: this.timeSlotCourse\n        });\n        this.courseName = '';\n        this.timeSlotCourse = '';\n        console.log(response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async addTimeSlot() {\n      try {\n        const response = await axios.post('http://localhost:5000/addTimeslot', {\n          timeSlot: this.timeSlot\n        });\n        this.timeSlot = '';\n        console.log(response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async addInstructor() {\n      try {\n        const response = await axios.post('http://localhost:5000/addInstructor', {\n          instructor: this.instructor,\n          courseNameInst: this.courseNameInst,\n          timeSlotInst: this.timeSlotInst\n        });\n        this.instructor = '';\n        this.courseNameInst = '';\n        this.timeSlotInst = '';\n        console.log(response.data);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    async schedule() {\n      try {\n        const response = await axios.get('http://localhost:5000/schedule');\n        if (response.data.success) {\n          this.responseMessage = response.data.message;\n          this.responseOutput = response.data.output;\n        } else {\n          this.responseMessage = response.data.message;\n        }\n      } \n      catch (error) {\n        this.responseMessage = 'Error occurred while scheduling';\n        console.error(error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n.container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  min-height: 95vh;\n  background-color: #ffffff69;\n}\n\n.form-grou